{
    "name": "iotyun/iot-tcp",
    "description": "TCP Internet of things communication server",
    "homepage": "http://www.iotyun.vip/",
    "keywords": [
        "NB-IOT",
        "huawei",
        "dianxin",
        "iot",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "iotyun",
            "email": "ytpay@qq.com",
            "homepage": "http://www.iotyun.vip/"
        }
    ],

    "support": {
        "email": "ytpay@qq.com"
    },
    "require": {
        "php": ">=7.1.0",
        "ext-openssl": "*",
        "topthink/framework": "^6.0.0",
        "ext-fileinfo": "*",
        "workerman/gateway-worker": "^3.0.0",
        "workerman/gatewayclient": "^3.0.13"
        
    },
	"autoload": {
        "psr-4": {
            "iotyun\\iot-tcp\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "iotyun\\iot-tcp\\Service"
            ],
            "config":{
                "worker": "src/config/worker.php",
                "worker_server": "src/config/server.php",
                "gateway_worker": "src/config/gateway.php"
            }
        }
    },

    "minimum-stability": "dev"
}
